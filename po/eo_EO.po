msgid ""
	"%s: Hide/Unhide a directory\n\n"
	"-h, --hide     Hide the directory\n"
	"-u, --unhide   Unhide the directory\n"
	"-l, --list     List the hidden directories\n"
	"-f, --flush    Flush the hide list\n"
	"    --version  Show the program version\n"
	"    --help     Show this message\n"
msgstr ""
	"%s: Kasxas/Montras la enhavon\n\n"
	"-h, --hide     Kasxi la enhavon\n"
	"-u, --unhide   Montri la enhavon\n"
	"-l, --list     Listi la kasxigitaj enhavojn\n"
	"-f, --flush    Malplenigi la liston de la kasxigitaj enhavoj\n"
	"    --version  Elskribi la varianton de la programo\n"
	"    --help     Aperigi cxi sciigon\n"

