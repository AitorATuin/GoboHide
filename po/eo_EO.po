msgid "%s is neither a directory nor a symbolic link\n"
msgstr "La %s estas ne enhavo, kaj ne simbola linko\n"


msgid "No inodes being hidden\n"
msgstr "Ni ne kasxas Inodojn\n"


msgid "Must be superuser\n"
msgstr "Vi devas esti system-administratoro\n"


msgid "Hidden directories:\n"
msgstr "Kasxigitaj enhavoj:\n"


msgid ""
